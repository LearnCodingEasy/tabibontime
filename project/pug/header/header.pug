                li(class="profile-menu-li logged-user")
                  a(class="profile-menu-a profile-logOut" data-href="createPost" href="#") 
                    i(class="fas fa-plus profile-menu-icon")
                    span(class="profile-menu-text") Create Post
                li(class="profile-menu-li logged-user")
                  a(class="profile-menu-a logout profile-logOut" data-href="logout" href="#") 
                    i(class="fas fa-sign-out-alt profile-menu-icon")
                    span(class="profile-menu-text") Log Out
                li(class="profile-menu-li not-logged-user")
                  a(class="profile-menu-a  profile-signUp" data-href="signUp" href="#") 
                    i(class="fas fa-sign-in-alt profile-menu-icon")
                    span(class="profile-menu-text") Sign Up
                li(class="profile-menu-li not-logged-user")
                  a(class="profile-menu-a  profile-LogIn" data-href="signIn" href="#") 
                    i(class="fas fa-sign-in-alt profile-menu-icon")
                    span(class="profile-menu-text") Log In
                // Theme  
                li(class="profile-menu-li profile-menu-li-theme") 
                  ul(class="profile-menu-li-theme-ul")
                    li(class="profile-menu-li-theme-ul-li active-theme" data-theme-name="light-theme")
                      span(class="profile-menu-li-theme-data-text") Light
                      i(class="fas fa-sun profile-menu-li-theme-icon")
                    li(class="profile-menu-li-theme-ul-li" data-theme-name="dark-theme")
                      span(class="profile-menu-li-theme-data-text") Dark
                      i(class="fas fa-moon profile-menu-li-theme-icon")
