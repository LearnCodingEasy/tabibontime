header(class="header" id="header")
  nav(class="navbar" id="navbar")
    div(class="navbar-container container")

      // Logo
      div(class="navbar-logo-div")
        a(href="index.html" class="navbar-logo-a")
          img(src="images/icons/icon.webp" class="navbar-logo-image" alt="Navbar Logo")
          span(class="navbar-logo-text")
            i(class="fas fa-stethoscope navbar-logo-text-icon")
            span(class="navbar-logo-text-content") Tabib On Line

      // Menu
      div(class="navbar-menu-div" id="navbar-menu-div")
        ul(class="navbar-menu-list-ul")
          li(class="navbar-menu-list-li")
            a(href="index.html" class="navbar-menu-list-a")
              span(class="navbar-menu-list-text") Home 
              i(class="fas fa-stethoscope navbar-menu-list-icon")
          li(class="navbar-menu-list-li")
            a(href="about.html" class="navbar-menu-list-a")
              span(class="navbar-menu-list-text") About 
              i(class="fas fa-address-card navbar-menu-list-icon")
          li(class="navbar-menu-list-li")
            a(href="services.html" class="navbar-menu-list-a")
              span(class="navbar-menu-list-text") services 
              i(class="fab fa-servicestack navbar-menu-list-icon")
          li(class="navbar-menu-list-li")
            a(href="index.html" class="navbar-menu-list-a")
              span(class="navbar-menu-list-text") Team 
              i(class="fas fa-file-signature navbar-menu-list-icon")
          li(class="navbar-menu-list-li")
            a(href="contact.html" class="navbar-menu-list-a")
              span(class="navbar-menu-list-text") contact 
              i(class="fas fa-file-signature navbar-menu-list-icon")

      // Profile Menu
      div(class="profile-menu")
        div(class="profile-menu-wrapper")
          div(class="profile-menu-inner")
            label(for="profile2" class="profile-menu-label-parent")
              input(type="checkbox" id="profile2" class="profile-menu-input-checkbox")
              img(src="https://cdn0.iconfinder.com/data/icons/avatars-3/512/avatar_hipster_guy-512.png" class="profile-menu-image" alt="Profile Menu Image")
              span(class="profile-menu-name") 
              label(for="profile2")
                i(class="fas fa-bars") 
              ul(class="profile-menu-ul")
                li(class="profile-menu-li logged-user")
